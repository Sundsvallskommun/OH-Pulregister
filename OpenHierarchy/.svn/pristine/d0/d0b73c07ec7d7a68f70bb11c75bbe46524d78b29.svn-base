package se.unlogic.hierarchy.core.settings;

import java.util.List;


public class MultiCheckboxSetting extends MultiValueSetting {

	public MultiCheckboxSetting(String id, String name, String description, List<Alternative> alternatives, List<String> defaultValues, boolean required) {

		this(id, name, description, alternatives, defaultValues, required, false);
	}
	
	public MultiCheckboxSetting(String id, String name, String description, List<Alternative> alternatives, List<String> defaultValues, boolean required, boolean disabled) {

		super(id, name, description, FormElement.CHECK, alternatives, defaultValues, required, disabled);
	}

}
